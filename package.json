{
  "name": "@justfolkert/just-art",
  "version": "0.0.1",
  "description": "A set of tooling that makes rendering high quality art in a webbrowser trivial",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "watch": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "tsc"
  },
  "keywords": [
    "graphics",
    "drawing",
    "rendering"
  ],
  "author": "Folkert Hoogenraad (JustF)",
  "license": "MIT",
  "devDependencies": {
    "tsup": "^8.3.5",
    "typescript": "^5.6.3"
  }
}
