{
  "compilerOptions": {
    "target": "es2018",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noUncheckedIndexedAccess": false,
    "noEmit": true
  },
  "exclude": [
    "node_modules",
    "**/dist/**",
  ]

}
